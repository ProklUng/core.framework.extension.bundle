{
  "name": "proklung/core-framework-extension-bundle",
  "description": "Core framework bundle for custom Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Gavrilov Fedy",
      "email": "fedor.gavrilov.75@mail.ru"
    }
  ],

  "support": {
    "issues": "https://github.com/ProklUng/core.framework.extension.bundle/issues",
    "source": "https://github.com/ProklUng/core.framework.extension.bundle"
  },
  "autoload": {
    "psr-4": {
      "Prokl\\CustomFrameworkExtensionsBundle\\": ""
    },
    "files": [
      "helpers.php"
    ]
  },
  "require": {
    "php": ">=7.2 | ^8.0",
    "symfony/dependency-injection": "^4.4 || ^5.0",
    "symfony/http-kernel": "^4.4 || ^5.0",
    "symfony/config": "^4.4 || ^5.0",
    "symfony/framework-bundle": "^4.4 || ^5.0",
    "symfony/filesystem": "^4.4 || ^5.0",
    "symfony/routing": "^4.4 || ^5.0",
    "symfony/http-foundation": "^4.4 || ^5.0",
    "symfony/event-dispatcher": "^4.4 || ^5.0",
    "symfony/property-access": "^4.4 || ^5.0",
    "symfony/serializer": "^4.4 || ^5.0",
    "symfony/validator": "^4.4 || ^5.0",
    "symfony/console": "^4.4 || ^5.0",
    "symfony/cache": "^4.4 || ^5.0",
    "symfony/yaml": "^4.4 || ^5.0",
    "symfony/messenger": "^4.4 || ^5.0",
    "symfony/amqp-messenger": "^4.4 || ^5.0",
    "symfony/redis-messenger": "^4.4 || ^5.0",
    "symfony/security-csrf": "^4.4 || ^5.0",
    "symfony/property-info": "^4.4 || ^5.0",
    "symfony/mailer": "^4.4 || ^5.0",
    "symfony/google-mailer": "^4.4 || ^5.0",
    "symfony/mime": "^4.4 || ^5.0",
    "symfony/notifier": "^5.0",
    "symfony/monolog-bridge": "^4.4 || ^5.0",
    "symfony/lock": "^4.4 || ^5.0",
    "symfony/stopwatch": "^4.4|^5.0",
    "symfony/http-client": "^4.4|^5.0",
    "php-http/httplug": "^2.2",
    "doctrine/annotations": "^1.10",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/orm": "^2.7",
    "doctrine/common": "^3.0",
    "doctrine/persistence": "^2.0",
    "doctrine/dbal": "2.13.1",
    "doctrine/inflector": "^1.4",
    "doctrine/cache": "^1.10",
    "symfony/polyfill-php80": "^1.20",
    "symfony/polyfill-php81": "^1.22",
    "symfony/twig-bridge": "^4.4 || ^5.0",
    "symfony/expression-language": "^5.2",
    "spiral/attributes": "^2.8",
    "league/html-to-markdown": "^5.0"
  },
  "require-dev": {
    "proklung/phpunit-testing-tools": "^1.5",
    "icanhazstring/composer-unused": "^0.7.5"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "phpstan/extension-installer": true,
      "icanhazstring/composer-unused": true
    }
  }
}
